var h=sn.uimgUrl+"/uimg/b2c/newcatentries/"+sn.supplierCode+"-"+f+"_1_100x100.jpg";
$("#proImg").attr("src",h);
var g=SubCode.getItemDisplayName(f);
productName=g;
$("#productName").html(g.length>18?g.substring(0,18)+"...":g);
$("#itemDisplayName").html(g);
$("#fourPageTitle").html(g)
}else{if(c!=""){for(var e in sn.subInfo[1].value){var b=sn.subInfo[1].value[e].characterValueId;
for(var d in sn.gInfo[0]){if(sn.gInfo[0][d].characterValueId==c&&d.substring(sn.gInfo[0][d].characterValueId.length)==b){var f=sn.gInfo[0][d].partNumber;
var h=sn.uimgUrl+"/uimg/b2c/newcatentries/"+sn.supplierCode+"-"+f+"_1_100x100.jpg";
$("#proImg").attr("src",h);
break
}}}}}}else{if(c!=""){var f=SubCode.getPartNumber(c,"");
var h=sn.uimgUrl+"/uimg/b2c/newcatentries/"+sn.supplierCode+"-"+f+"_1_100x100.jpg";
$("#proImg").attr("src",h);
var g=SubCode.getItemDisplayName(f);
$("#productName").html(g.length>18?g.substring(0,18)+"...":g);
$("#itemDisplayName").html(g);
$("#fourPageTitle").html(g)
}}};
SubCode.refreshRelatePrice=function(b,a,f){if(sn.subInfo.length==2){if(b!=""&&a!=""){var e=SubCode.getPartNumber(b,a);
var d="";
