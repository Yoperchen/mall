}function uuid(){var d=[];
var a="0123456789abcdef";
for(var b=0;
b<36;
b++){d[b]=a.substr(Math.floor(Math.random()*16),1)
}d[14]="4";
d[19]=a.substr((d[19]&3)|8,1);
d[8]=d[13]=d[18]=d[23]="";
var c=d.join("");
return c
