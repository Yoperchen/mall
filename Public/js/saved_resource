}function randomString(b){var c="ABCDEFGHJKMNPQRSTWXYZabcdefhijkmnprstwxyz2345678";
var a=c.length;
var d="";
for(i=0;
i<b;
i++){d+=c.charAt(Math.floor(Math.random()*a))
}return d
}function refreshImageCode(a){sn.imageMsg="";
