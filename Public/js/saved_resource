}}if(h[d].invStatus=="1"||h[d].invStatus=="2"||h[d].invStatus=="4"||h[d].invStatus=="5"||h[d].invStatus=="6"||h[d].invStatus=="7"){g=sn.clusterMap[l].itemCuPartNumber[f].partNumber;
m=true;
break
}}}}if(m){break
}}}if(m){break
}}}}if(!m){g=n
}return g
};
var SubCode=SubCode||{};
SubCode.render=function(b,c){for(var a in b){if(b[a].isHide){$(".items").eq(0).find("label[colorid='"+b[a].colorid+"']").removeClass("checked").removeClass("disabled").hide()
}else{if(b[a].isVoid){$(".items").eq(0).find("label[colorid='"+b[a].colorid+"']").removeClass("checked").addClass("disabled").show()
}else{$(".items").eq(0).find("label[colorid='"+b[a].colorid+"']").removeClass("disabled").show()
}}}for(var a in c){if(c[a].isHide){$(".items").eq(1).find("label[colorid='"+c[a].versionid+"']").removeClass("checked").removeClass("disabled").hide()
}else{if(c[a].isVoid){$(".items").eq(1).find("label[colorid='"+c[a].versionid+"']").removeClass("checked").addClass("disabled").show()
}else{$(".items").eq(1).find("label[colorid='"+c[a].versionid+"']").removeClass("disabled").show()
}}}};
SubCode.getPartNumber=function(b,a){var d="";
for(var c in sn.gInfo[0]){if(sn.gInfo[0][c].characterValueId==b&&c.substring(sn.gInfo[0][c].characterValueId.length)==a){d=sn.gInfo[0][c].partNumber;
break
}}return d
};
SubCode.getItemDisplayName=function(b){var c="";
for(var a in sn.gInfo[0]){if(sn.gInfo[0][a].partNumber==b){c=sn.gInfo[0][a].itemDisplayName;
break
}}return c
};
SubCode.getColorid=function(b){var c="";
for(var a in sn.gInfo[0]){if(sn.gInfo[0][a].partNumber==b){c=sn.gInfo[0][a].characterValueId;
find=true;
break
}}return c
};
SubCode.getVersionid=function(c){var a="";
for(var b in sn.gInfo[0]){if(sn.gInfo[0][b].partNumber==c){a=b.substring(sn.gInfo[0][b].characterValueId.length);
find=true;
break
}}return a
};
SubCode.selectText="";
SubCode.getSelectText=function(e,d){var f=SubCode.getColorid(e);
var a=SubCode.getVersionid(e);
var b="";
if(SubCode.selectText==""){if(f!=""||a!=""){SubCode.selectText="选择:";
if(f!=""){for(var c in sn.subInfo[0].value){if(sn.subInfo[0].value[c].characterValueId==f){SubCode.selectText+=" "+sn.subInfo[0].value[c].characterValueDisplayName;
break
}}}if(a!=""){for(var c in sn.subInfo[1].value){if(sn.subInfo[1].value[c].characterValueId==a){SubCode.selectText+=" "+sn.subInfo[1].value[c].characterValueDisplayName;
break
}}}}else{SubCode.selectText="选择";
for(var c in sn.subInfo){SubCode.selectText+=" "+sn.subInfo[c].key
}}}if(SubCode.selectText.indexOf("选择:")<0){b=SubCode.selectText
}else{b=SubCode.selectText+" "+d+"件"
}return b
};
SubCode.isShow=function(a){if(sn.isCShop=="1"){if(a.invStatus=="1"){return true
}}else{if(a.invStatus=="1"||a.invStatus=="4"||a.invStatus=="5"||a.invStatus=="6"||a.invStatus=="7"){return true
}}return false
};
SubCode.isShowPartNumber=function(b,c){for(var a in c.price.saleInfo){if(c.price.saleInfo[a].partNumber==b){if(SubCode.isShow(c.price.saleInfo[a])){return true
}break
}}return false
};
SubCode.isHideColor=function(d,c){isHide=true;
for(var a in sn.gInfo[0]){if(sn.gInfo[0][a].characterValueId==d){var b=sn.gInfo[0][a].partNumber;
if(SubCode.isShowPartNumber(b,c)){isHide=false;
break
}}}return isHide
};
SubCode.isHideVersion=function(a,d){isHide=true;
for(var b in sn.gInfo[0]){if(b.substring(sn.gInfo[0][b].characterValueId.length)==a){var c=sn.gInfo[0][b].partNumber;
if(SubCode.isShowPartNumber(c,d)){isHide=false;
break
}}}return isHide
};
SubCode.isAllHide=function(b,c){for(var a in b){if(!b[a].isHide){return false
}}for(var a in c){if(!c[a].isHide){return false
}}return true
};
SubCode.refreshTM=function(c,g,e){if(sn.subInfo.length==2){var h=[];
for(var f in sn.subInfo[0].value){var b=sn.subInfo[0].value[f].characterValueId;
h[f]={};
h[f].colorid=b;
h[f].isHide=SubCode.isHideColor(b,e)
}var d=[];
for(var f in sn.subInfo[1].value){var a=sn.subInfo[1].value[f].characterValueId;
d[f]={};
d[f].versionid=a;
d[f].isHide=SubCode.isHideVersion(a,e)
}if(SubCode.isAllHide(h,d)){for(var f in h){h[f].isHide=false;
h[f].isVoid=true
}for(var f in d){d[f].isHide=false;
d[f].isVoid=true
}}if(c!=""){for(var j in sn.gInfo[0]){if(sn.gInfo[0][j].characterValueId==c){var k=sn.gInfo[0][j].partNumber;
if(!SubCode.isShowPartNumber(k,e)){var a=j.substring(sn.gInfo[0][j].characterValueId.length);
for(var f in d){if(d[f].versionid==a){d[f].isHide=true
}}}}}}SubCode.render(h,d)
}else{if(sn.subInfo.length==1){var h=[];
for(var f in sn.subInfo[0].value){var b=sn.subInfo[0].value[f].characterValueId;
h[f]={};
h[f].colorid=b;
h[f].isHide=SubCode.isHideColor(b,e)
}var d=[];
if(SubCode.isAllHide(h,d)){for(var f in h){h[f].isHide=false;
h[f].isVoid=true
}}SubCode.render(h,d)
}}};
SubCode.renderPopupPrice=function(d,c){if(d!=""){var a="";
var b={};
if(d!=""){Price.setPrice(b,d);
if(b.isRange){a=b.priceLow+b.priceLowDec+" - "+b.priceHigh+b.priceHighDec
}else{if(b.isNum){a=b.price+b.priceDec
}else{if(b.isText){a=b.price
}}}}$("#choosePrice").html("<i>&yen;</i>"+a+"<span>商品编码:"+c.substring(9,18)+"</span>")
}else{$("#choosePrice").html("暂不销售<span>商品编码:"+c.substring(9,18)+"</span>")
}};
SubCode.priceTM="";
SubCode.refreshPopupPriceTM=function(g){var c=/^\d+(\.\d+)?$/;
var k=false;
var d=0;
var b="";
var l=0;
var f="";
var a="";
for(var m in sn.gInfo[0]){var o=sn.gInfo[0][m].partNumber;
for(var e in g.price.saleInfo){if(g.price.saleInfo[e].partNumber==o&&SubCode.isShow(g.price.saleInfo[e])){if(c.test(g.price.saleInfo[e].promotionPrice)){var h=parseFloat(g.price.saleInfo[e].promotionPrice);
if(!k){d=h;
b=g.price.saleInfo[e].promotionPrice;
l=h;
f=g.price.saleInfo[e].promotionPrice;
k=true
}if(h>d){d=h;
b=g.price.saleInfo[e].promotionPrice
}if(h<l){l=h;
f=g.price.saleInfo[e].promotionPrice
}}else{if(a==""){a=g.price.saleInfo[e].promotionPrice
}}}}}var n="";
if(k){if(b==f){n=f
}else{n=f+"-"+b
}}else{if(a!=""){n=a
}}SubCode.renderPopupPrice(n,sn.partNumber);
SubCode.priceTM=n
};
SubCode.refreshPopupPrice=function(f,m,k){var c=/^\d+(\.\d+)?$/;
var n=false;
var e=0;
var b="";
var o=0;
var h="";
var a="";
for(var p in sn.gInfo[0]){if((f!=""&&sn.gInfo[0][p].characterValueId==f)||(m!=""&&p.substring(sn.gInfo[0][p].characterValueId.length)==m)){var r=sn.gInfo[0][p].partNumber;
for(var g in k.price.saleInfo){if(k.price.saleInfo[g].partNumber==r&&SubCode.isShow(k.price.saleInfo[g])){if(c.test(k.price.saleInfo[g].promotionPrice)){var l=parseFloat(k.price.saleInfo[g].promotionPrice);
if(!n){e=l;
b=k.price.saleInfo[g].promotionPrice;
o=l;
h=k.price.saleInfo[g].promotionPrice;
n=true
}if(l>e){e=l;
b=k.price.saleInfo[g].promotionPrice
}if(l<o){o=l;
h=k.price.saleInfo[g].promotionPrice
}}else{if(a==""){a=k.price.saleInfo[g].promotionPrice
}}}}}}var q="";
var d="";
if(n){if(b==h){q=h
}else{q=h+"-"+b
}}else{if(a!=""){q=a
}else{q=SubCode.priceTM
}}SubCode.renderPopupPrice(q,sn.partNumber)
};
