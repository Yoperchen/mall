}function accSub(f,d){var c,b,a,h;
try{c=f.toString().split(".")[1].length
}catch(g){c=0
}try{b=d.toString().split(".")[1].length
}catch(g){b=0
}a=Math.pow(10,Math.max(c,b));
h=(c>=b)?c:b;
return((d*a-f*a)/a).toFixed(h)
}function initCartNum(){var a=0;
probeAuthStatus(function(){mtisAbTestCook=$.cookie("mtisAbTest");
if("B"==mtisAbTestCook){a=(parseInt($.cookie("mtisCartQty"))||0)
}else{a=(parseInt($.cookie("totalProdQtyv3"))||0)+(parseInt($.cookie("mtisCartQty"))||0)
}if(a>99){$("#cartNum").html("99+");
$("#cartNum").show()
}else{if(a>0){$("#cartNum").html(a);
$("#cartNum").show()
}else{$("#cartNum").html("");
$("#cartNum").hide()
}}},function(){a=(parseInt($.cookie("totalProdQtyv3"))||0)+(parseInt($.cookie("mtisCartQty"))||0);
if(a>99){$("#cartNum").html("99+");
$("#cartNum").show()
}else{if(a>0){$("#cartNum").html(a);
$("#cartNum").show()
}else{$("#cartNum").html("");
$("#cartNum").hide()
}}})
