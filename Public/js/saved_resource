}var Price=Price||{};
Price.render=function(b){var a=template("price_tmpl",b);
$("#pricemain").html(a);
if(sn.gType=="s"&&sn.itemType=="20"&&sn.openTMpage=="1"){$("#pricemain").removeClass("inline-price")
}else{$("#pricemain").addClass("inline-price")
}};
Price.splitDec=function(e){var b=[];
var d=/^\d+(\.\d+)?$/;
var a=/^\d+(\.\d+)-\d+(\.\d+)?$/;
if(d.test(e)){b[0]={};
numArr=e.split(".");
if(numArr.length==2){b[0].num=numArr[0];
b[0].dec="."+numArr[1]
}else{if(numArr.length==1){b[0].num=numArr[0];
b[0].dec=""
}else{b[0].num="";
b[0].dec=""
}}}else{if(a.test(e)){rangeArr=e.split("-");
for(var c in rangeArr){numArr=rangeArr[c].split(".");
b[c]={};
if(numArr.length==2){b[c].num=numArr[0];
b[c].dec="."+numArr[1]
}else{if(numArr.length==1){b[c].num=numArr[0];
b[c].dec=""
}else{b[c].num="";
b[c].dec=""
}}}}}return b
};
Price.setPrice=function(d,a,c){var b=Price.splitDec(a);
if(b.length==2){d.isRange=true;
d.priceLow=b[0].num;
d.priceHigh=b[1].num;
if(c){if(b[0].dec==".00"&&b[1].dec==".00"){d.priceLowDec="";
d.priceHighDec=""
}else{d.priceLowDec=b[0].dec;
d.priceHighDec=b[1].dec
}}else{d.priceLowDec=b[0].dec;
d.priceHighDec=b[1].dec
}}else{if(b.length==1){d.isNum=true;
d.price=b[0].num;
if(c){if(b[0].dec==".00"){d.priceDec=""
}else{d.priceDec=b[0].dec
}}else{d.priceDec=b[0].dec
}}else{d.isText=true;
d.price=a
}}};
Price.setPriceRef=function(b,c,a){var d=Price.splitDec(c);
if(d.length==2){b.isRangeRef=true;
b.priceRefLow=d[0].num;
b.priceRefHigh=d[1].num;
if(a){if(d[0].dec==".00"&&d[1].dec==".00"){b.priceRefLowDec="";
b.priceRefHighDec=""
}else{b.priceRefLowDec=d[0].dec;
b.priceRefHighDec=d[1].dec
}}else{b.priceRefLowDec=d[0].dec;
b.priceRefHighDec=d[1].dec
}}else{if(d.length==1){b.isNumRef=true;
b.priceRef=d[0].num;
if(a){if(d[0].dec==".00"){b.priceRefDec=""
}else{b.priceRefDec=d[0].dec
}}else{b.priceRefDec=d[0].dec
}}else{b.isTextRef=true;
b.priceRef=c
}}};
Price.isHideRef=function(c){if(c.isText||c.isTextRef){return false
}var a=0;
var d=0;
if(c.isRange){a=parseFloat(c.priceLow+c.priceLowDec);
d=parseFloat(c.priceHigh+c.priceHighDec)
}else{if(c.isNum){a=parseFloat(c.price+c.priceDec);
d=parseFloat(c.price+c.priceDec)
}}var e=0;
var b=0;
if(c.isRangeRef){e=parseFloat(c.priceRefLow+c.priceRefLowDec);
b=parseFloat(c.priceRefHigh+c.priceRefHighDec)
}else{if(c.isNumRef){e=parseFloat(c.priceRef+c.priceRefDec);
b=parseFloat(c.priceRef+c.priceRefDec)
}}if(e<=a||b<=d){return true
}else{return false
}};
Price.initTM=function(a){Price.init(a,true)
};
Price.init=function(c,a){var b={};
if(sn.categoryCode_mdm&&sn.productGroup&&sn.productGroup.indexOf(sn.categoryCode_mdm)!=-1){b.forbid=true
}else{b.forbid=false;
if(sn.priceType!="4-1"&&sn.priceType!="4-4"&&sn.priceType!="4-12"){if(sn.hasStorage=="Z"&&c.djhFlag!="1"&&c.promotionPrice==""){b.hasPrice=false
}else{b.hasPrice=true;
if(sn.isBook=="false"){if(c.priceType=="0"){Price.setPrice(b,c.promotionPrice,a);
if(c.refPrice&&c.refPrice!=""){b.drawRef=true;
Price.setPriceRef(b,c.refPrice,a);
if(Price.isHideRef(b)){b.drawRef=false
}}else{b.drawRef=false
}if(sn.gType=="s"&&sn.itemType=="20"&&sn.openTMpage=="1"){b.drawLabelTM=true
}else{b.drawLabel=true
}if(sn.hasStorage=="Y"){if(sn.gType=="s"&&sn.itemType=="20"&&sn.openTMpage=="1"){}else{b.drawNoticeJJ=true
}}}else{Price.setPrice(b,c.promotionPrice,a);
if(c.refPrice&&c.refPrice!=""){b.drawRef=true;
Price.setPriceRef(b,c.refPrice,a);
if(Price.isHideRef(b)){b.drawRef=false
}}else{if(c.netPrice&&c.netPrice!=""){b.drawRef=true;
Price.setPriceRef(b,c.netPrice,a);
if(Price.isHideRef(b)){b.drawRef=false
}}else{b.drawRef=false
}}if(sn.gType=="s"&&sn.itemType=="20"&&sn.openTMpage=="1"){b.drawLabelTM=true
}else{b.drawLabel=true
}}}else{Price.setPrice(b,c.promotionPrice,a);
if(c.itemPrice&&c.itemPrice!=""){b.drawRef=true;
Price.setPriceRef(b,c.itemPrice,a);
if(Price.isHideRef(b)){b.drawRef=false
}if(sn.gType=="s"&&sn.itemType=="20"&&sn.openTMpage=="1"){b.drawLabelTM=true
}else{b.drawLabel=true
}}else{b.drawRef=false
}}}}}Price.render(b)
};
function priceArrParam(c,e){var f={};
f.url=sn.icpsDomain;
var a="";
var g=sn.supplierCode;
var d="";
for(var b in c){if(a!=""){a+=",";
d+=","
}a+=c[b];
d+=g
}f.cmmdtyCode=a;
f.cityCode=sn.cityCode;
f.area=sn.cityCode+sn.districtCode+"01";
f.bizCode=d;
f.chan="5";
f.callback=e;
return f
}function getClusterPriceArr(c,d,a){var b=c.url+"/icps-web/getVarnishAllPrice014/"+c.cmmdtyCode+"_"+c.cityCode+"_"+c.area+"_"+c.bizCode+"_"+c.chan+"_"+c.callback+".vhtm";
$.ajax({url:b,dataType:"jsonp",jsonp:"callback",jsonpCallback:c.callback,cache:true,error:function(e,g,f){if(a){a()
}},success:function(e){if(d){d(e)
}}})
}var paraArrLeft=[];
var clusterPriceData=[];
function colorClusterArrCallback(d){for(var b in d){clusterPriceData.push(d[b])
}var c=[];
if(paraArrLeft.length>20){leftArr=[];
for(var b=0;
b<paraArrLeft.length;
b++){if(b<=19){c.push(paraArrLeft[b])
}else{leftArr.push(paraArrLeft[b])
}}paraArrLeft=leftArr
}else{c=paraArrLeft;
paraArrLeft=[]
}if(c.length>0){getClusterPriceArr(priceArrParam(c,"colorClusterArrCallback"))
}else{var e=$("#colorItemList label[class=checked]").attr("colorid");
var a="";
if(sn.versionInfo.length>0){a=$("#versionItemList label[class=checked]").attr("versionid")
}ColorCluster.refresh(e,a,clusterPriceData)
}}function setColorCluster(){var a=[];
if(sn.colorInfo.length>0&&sn.versionInfo.length>0){for(var c in sn.clusterMap){for(var b in sn.clusterMap[c].itemCuPartNumber){a.push(sn.clusterMap[c].itemCuPartNumber[b].partNumber)
}}}else{for(var c in sn.colorInfo){a.push(sn.colorInfo[c].partNumber)
}}ClusterPriceData=[];
paraArrLeft=a;
data=[];
colorClusterArrCallback(data)
}var ColorCluster=ColorCluster||{};
ColorCluster.selectText="";
ColorCluster.getSelectText=function(b,c){var a="";
if(ColorCluster.selectText==""){$(".select-list").find("label[class=checked]").each(function(){if($(this).attr("data-name")){ColorCluster.selectText+=" "+$(this).attr("data-name")
}})
}c=c?" "+c:"";
if(b==""){a="选择: "+ColorCluster.selectText+c
}else{a="选择: "+ColorCluster.selectText+c+" "+b+"件"
}return a
};
ColorCluster.render=function(b,c){for(var a in b){if(b[a].isHide){$(".items").eq(0).find("label[colorid='"+b[a].colorid+"']").removeClass("checked").removeClass("actived").removeClass("disabled").hide()
}else{if(b[a].isVoid){$(".items").eq(0).find("label[colorid='"+b[a].colorid+"']").removeClass("actived").addClass("disabled").show()
}else{if(b[a].isVoidActive){$(".items").eq(0).find("label[colorid='"+b[a].colorid+"']").addClass("disabled").addClass("actived").show()
}else{$(".items").eq(0).find("label[colorid='"+b[a].colorid+"']").removeClass("disabled").removeClass("actived").show()
}}}}for(var a in c){if(c[a].isHide){$(".items").eq(1).find("label[versionid='"+c[a].versionid+"']").removeClass("checked").removeClass("actived").removeClass("disabled").hide()
}else{if(c[a].isVoid){$(".items").eq(1).find("label[versionid='"+c[a].versionid+"']").removeClass("actived").addClass("disabled").show()
}else{if(c[a].isVoidActive){$(".items").eq(1).find("label[versionid='"+c[a].versionid+"']").addClass("disabled").addClass("actived").show()
}else{$(".items").eq(1).find("label[versionid='"+c[a].versionid+"']").removeClass("disabled").removeClass("actived").show()
}}}}};
ColorCluster.init=function(){if(sn.colorInfo.length>0&&sn.versionInfo.length>0){$(".items").eq(0).find("label").removeClass("disabled").removeClass("actived").removeClass("checked");
$(".items").eq(1).find("label").removeClass("disabled").removeClass("actived").removeClass("checked");
for(var d in sn.clusterMap){for(var c in sn.clusterMap[d].itemCuPartNumber){if(sn.clusterMap[d].itemCuPartNumber[c].partNumber==sn.partNumber){var b=sn.clusterMap[d].color;
var a=sn.clusterMap[d].itemCuPartNumber[c].versionId;
$(".items").eq(0).find("label[colorid='"+b+"']").removeClass("disabled").removeClass("actived").addClass("checked").show();
$(".items").eq(1).find("label[versionid='"+a+"']").removeClass("disabled").removeClass("actived").addClass("checked").show();
return
}}}}else{$(".items").eq(0).find("label").removeClass("disabled").removeClass("actived").removeClass("checked");
for(var d in sn.colorInfo){if(sn.colorInfo[d].partNumber==sn.partNumber){var b=sn.colorInfo[d].characterValueId;
$(".items").eq(0).find("label[colorid='"+b+"']").removeClass("disabled").removeClass("actived").addClass("checked").show();
return
}}}};
ColorCluster.isHideColor=function(d,c){isHide=true;
if(sn.colorInfo.length>0&&sn.versionInfo.length>0){for(var b in sn.clusterMap){if(sn.clusterMap[b].color==d){isHide=false;
break
}}}else{if(sn.colorInfo.length>0){for(var b in sn.colorInfo){if(sn.colorInfo[b].characterValueId==d){for(var a in c){if(c[a].cmmdtyCode==sn.colorInfo[b].partNumber){if(c[a].priceType.length>0){var e=c[a].priceType.substring(0,1);
if(e=="7"||e=="8"){isHide=false;
break
}}if(c[a].invStatus=="1"||c[a].invStatus=="2"||c[a].invStatus=="4"||c[a].invStatus=="5"||c[a].invStatus=="6"||c[a].invStatus=="7"){isHide=false;
break
}}}}}}}return isHide
};
ColorCluster.isVoidActiveColor=function(f,d,e){isVoidActive=true;
if(sn.colorInfo.length>0&&sn.versionInfo.length>0){for(var c in sn.clusterMap){if(sn.clusterMap[c].color==f){for(var b in sn.clusterMap[c].itemCuPartNumber){if(sn.clusterMap[c].itemCuPartNumber[b].versionId==d){for(var a in e){if(e[a].cmmdtyCode==sn.clusterMap[c].itemCuPartNumber[b].partNumber){if(e[a].priceType.length>0){var g=e[a].priceType.substring(0,1);
if(g=="7"||g=="8"){isVoidActive=false;
break
}}if(e[a].invStatus=="1"||e[a].invStatus=="4"||e[a].invStatus=="5"||e[a].invStatus=="6"||e[a].invStatus=="7"){isVoidActive=false;
break
}}}}}}}}else{if(sn.colorInfo.length>0){for(var c in sn.colorInfo){if(sn.colorInfo[c].characterValueId==f){for(var a in e){if(e[a].cmmdtyCode==sn.colorInfo[c].partNumber){if(e[a].priceType.length>0){var g=e[a].priceType.substring(0,1);
if(g=="7"||g=="8"){isVoidActive=false;
break
}}if(e[a].invStatus=="1"||e[a].invStatus=="3"||e[a].invStatus=="4"||e[a].invStatus=="5"||e[a].invStatus=="6"||e[a].invStatus=="7"){isVoidActive=false;
break
}}}}}}}return isVoidActive
};
ColorCluster.isHideVersion=function(b,e,f){isHide=true;
for(var d in sn.clusterMap){if(sn.clusterMap[d].color==e){for(var c in sn.clusterMap[d].itemCuPartNumber){if(sn.clusterMap[d].itemCuPartNumber[c].versionId==b){for(var a in f){if(f[a].cmmdtyCode==sn.clusterMap[d].itemCuPartNumber[c].partNumber){if(f[a].priceType.length>0){var g=f[a].priceType.substring(0,1);
if(g=="7"||g=="8"){isHide=false;
break
}}if(f[a].invStatus=="1"||f[a].invStatus=="2"||f[a].invStatus=="4"||f[a].invStatus=="5"||f[a].invStatus=="6"||f[a].invStatus=="7"){isHide=false;
break
}}}}}}}return isHide
};
ColorCluster.isVoidActiveVersion=function(b,e,f){isVoidActive=true;
for(var d in sn.clusterMap){if(sn.clusterMap[d].color==e){for(var c in sn.clusterMap[d].itemCuPartNumber){if(sn.clusterMap[d].itemCuPartNumber[c].versionId==b){for(var a in f){if(f[a].cmmdtyCode==sn.clusterMap[d].itemCuPartNumber[c].partNumber){if(f[a].priceType.length>0){var g=f[a].priceType.substring(0,1);
if(g=="7"||g=="8"){isVoidActive=false;
break
}}if(f[a].invStatus=="1"||f[a].invStatus=="3"||f[a].invStatus=="4"||f[a].invStatus=="5"||f[a].invStatus=="6"||f[a].invStatus=="7"){isVoidActive=false;
break
}}}}}}}return isVoidActive
};
ColorCluster.refresh=function(c,a,g){if(sn.colorInfo.length>0&&sn.versionInfo.length>0){var e=[];
for(var d in sn.colorInfo){var f=sn.colorInfo[d].characterValueId;
e[d]={};
e[d].colorid=f;
if(f!=c){e[d].isHide=ColorCluster.isHideColor(f,g);
e[d].isVoidActive=ColorCluster.isVoidActiveColor(f,a,g)
}}var h=[];
for(var d in sn.versionInfo){var b=sn.versionInfo[d].characterValueId;
h[d]={};
h[d].versionid=b;
if(b!=a){h[d].isHide=ColorCluster.isHideVersion(b,c,g);
h[d].isVoidActive=ColorCluster.isVoidActiveVersion(b,c,g)
}}ColorCluster.render(e,h)
}else{var e=[];
for(var d in sn.colorInfo){var f=sn.colorInfo[d].characterValueId;
e[d]={};
e[d].colorid=f;
if(f!=c){e[d].isHide=ColorCluster.isHideColor(f,g);
e[d].isVoidActive=ColorCluster.isVoidActiveColor(f,a,g)
}}var h=[];
ColorCluster.render(e,h)
}};
ColorCluster.refreshHK=function(d){if(sn.colorInfo.length>0&&sn.versionInfo.length>0){var l=[];
for(var g in sn.colorInfo){var c=sn.colorInfo[g].characterValueId;
l[g]={};
l[g].colorid=c
}var f=[];
for(var g in sn.versionInfo){var a=sn.versionInfo[g].characterValueId;
f[g]={};
f[g].versionid=a;
var h=false;
for(var e in sn.clusterMap){if(sn.clusterMap[e].color==d){for(var b in sn.clusterMap[e].itemCuPartNumber){if(sn.clusterMap[e].itemCuPartNumber[b].versionId==a){h=true;
break
}}break
}}if(!h){f[g].isVoid=true
}}ColorCluster.render(l,f)
}else{var l=[];
for(var g in sn.colorInfo){var c=sn.colorInfo[g].characterValueId;
l[g]={};
l[g].colorid=c
}var f=[];
ColorCluster.render(l,f)
}};
ColorCluster.getNewPartNumber=function(e,a,n,h,b){var g="";
var m=false;
for(var l in sn.clusterMap){if(sn.clusterMap[l].color==e){for(var f in sn.clusterMap[l].itemCuPartNumber){if(sn.clusterMap[l].itemCuPartNumber[f].versionId==a){g=sn.clusterMap[l].itemCuPartNumber[f].partNumber;
m=true;
break
}}}if(m){break
}}if(!m){if(b){for(var l in sn.clusterMap){if(sn.clusterMap[l].color==e){for(var f in sn.clusterMap[l].itemCuPartNumber){if(sn.clusterMap[l].itemCuPartNumber[f].versionId!=a){for(var d in h){if(h[d].cmmdtyCode==sn.clusterMap[l].itemCuPartNumber[f].partNumber){if(h[d].priceType.length>0){var c=h[d].priceType.substring(0,1);
if(c=="7"||c=="8"){g=sn.clusterMap[l].itemCuPartNumber[f].partNumber;
m=true;
break
}}if(h[d].invStatus=="1"||h[d].invStatus=="2"||h[d].invStatus=="4"||h[d].invStatus=="5"||h[d].invStatus=="6"||h[d].invStatus=="7"){g=sn.clusterMap[l].itemCuPartNumber[f].partNumber;
m=true;
break
}}}}if(m){break
}}}if(m){break
}}}else{for(var l in sn.clusterMap){if(sn.clusterMap[l].color!=e){for(var f in sn.clusterMap[l].itemCuPartNumber){if(sn.clusterMap[l].itemCuPartNumber[f].versionId==a){for(var d in h){if(h[d].cmmdtyCode==sn.clusterMap[l].itemCuPartNumber[f].partNumber){if(h[d].priceType.length>0){var c=h[d].priceType.substring(0,1);
