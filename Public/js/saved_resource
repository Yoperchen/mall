}if(e.minute<10){e.minute="0"+e.minute
}if(e.hour<10){e.hour="0"+e.hour
}if(e.day<10){e.day="0"+e.day
}return[e.second,e.minute,e.hour,e.day]
},parseDate:function(f){var e=new Date(parseInt(f));
