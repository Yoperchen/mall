function getVendor(a){if(a.vendorCode&&a.vendorCode!=""){sn.supplierCode=a.vendorCode
}else{if(sn.shopType&&sn.shopType=="-1"){sn.supplierCode="0000000000"
}}sn.swlShopFlag="";
sn.hwgType="";
if(sn.shopType&&sn.shopType=="-1"){sn.vendorType=""
}if(a.vendorType==VendorConst.SHOP||a.vendorType==VendorConst.SWL||a.vendorType==VendorConst.CSSWL||a.vendorType==VendorConst.HWG||a.vendorType==VendorConst.HWG1){sn.isCShop="1";
sn.vendorType=a.vendorType;
if(a.vendorType==VendorConst.SWL||a.vendorType==VendorConst.CSSWL){sn.swlShopFlag="true"
}else{if(a.vendorType==VendorConst.HWG||a.vendorType==VendorConst.HWG1){if(a.ownerPlace.indexOf("H")==0){sn.hwgType="2"
}else{if(a.ownerPlace.indexOf("B")==0||a.ownerPlace.indexOf("L")==0){sn.hwgType="1"
}else{sn.hwgType="0"
}}}}}else{if(sn.vendorType=="1"||sn.vendorType=="2"||sn.vendorType=="3"||sn.vendorType=="4"||sn.vendorType==VendorConst.SHOP||sn.vendorType==VendorConst.SWL||sn.vendorType==VendorConst.HWG||sn.vendorType==VendorConst.HWG1||sn.vendorType==VendorConst.CSSWL){sn.isCShop="1";
if(sn.vendorType=="1"){sn.vendorType=VendorConst.SHOP
}else{if(sn.vendorType=="2"){sn.vendorType=VendorConst.SWL;
sn.swlShopFlag="true"
}else{if(sn.vendorType=="3"){if(a.ownerPlace.indexOf("H")==0){sn.vendorType=VendorConst.HWG;
sn.hwgType="2"
}else{if(a.ownerPlace.indexOf("B")==0||a.ownerPlace.indexOf("L")==0){sn.vendorType=VendorConst.HWG;
sn.hwgType="1"
}else{sn.vendorType=VendorConst.HWG1;
sn.hwgType="0"
}}}else{if(sn.vendorType=="4"){sn.vendorType=VendorConst.CSSWL;
sn.swlShopFlag="true"
}}}}}else{sn.isCShop="0"
}}}function initProductBaseInfoFromCallbackTM(a){isYuyd=false;
